{
  "name": "@bouzuya/tsv",
  "description": "tsv: tab-separated values",
  "version": "0.0.0",
  "author": "bouzuya <m@bouzuya.net> (https://bouzuya.net/)",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "bugs": {
    "url": "https://github.com/bouzuya/node-tsv/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.22",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier/@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "root": true,
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "files": [
    "lib",
    "!lib/**/*.test.*"
  ],
  "homepage": "https://github.com/bouzuya/node-tsv#readme",
  "keywords": [
    "tsv"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bouzuya/node-tsv.git"
  },
  "scripts": {
    "build": "run-s build:format build:lint build:typecheck build:dts build:js",
    "build:dts": "tsc --emitDeclarationOnly",
    "build:format": "prettier --check 'src/**/*.ts'",
    "build:js": "babel --extensions '.ts' --out-dir lib 'src/**/*.ts'",
    "build:lint": "eslint 'src/**/*.ts'",
    "build:typecheck": "tsc --noEmit",
    "clean": "rimraf lib",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "run-s clean build",
    "test": "jest"
  },
  "types": "lib/index.d.ts"
}
